<?xml version="1.0"?>
<!-- TAGS ARE CASE SENSATIVE -->
<LOGFORMAT>
  <MULTIWII>
<!-- Log fields from WinGUI logging-->
    <!-- Raw Gyro and Accel data-->
    <GRAW>
      <F1>AccX</F1>
      <F2>AccY</F2>
      <F3>AccZ</F3>
      <F4>Gyro X</F4>
      <F5>Gyro Y</F5>
      <F6>Gyro Z</F6>
      <F7>Pitch Angle</F7>
      <F8>Roll Angle</F8>
    </GRAW>
<!-- Attitude (angle Roll and Angle Pitch)-->
    <GATT>
      <F1>Pitch Angle</F1>
      <F2>Roll Angle</F2>
    </GATT>
    
    <GMAG>
      <F1>Mag X</F1>
      <F2>Mag Y</F2>
      <F3>Mag Z</F3>
      <F4>Heading</F4>
      <F5>Barometer</F5>
    </GMAG>
    <GRCC>
      <F1>RC Throttle</F1>
      <F2>RC Pitch</F2>
      <F3>RC Roll</F3>
      <F4>RC Yaw</F4>
    </GRCC>
    <GRCX>
      <F1>RC AUX1</F1>
      <F2>RC AUX2</F2>
      <F3>RC AUX3</F3>
      <F4>RC AUX4</F4>
    </GRCX>
    <GMOT>
      <F1>Motor 1</F1>
      <F2>Motor 2</F2>
      <F3>Motor 3</F3>
      <F4>Motor 4</F4>
      <F5>Motor 5</F5>
      <F6>Motor 6</F6>
      <F7>Motor 7</F7>
      <F8>Motor 8</F8>
    </GMOT>
    <GSRV>
      <F1>Servo 1</F1>
      <F2>Servo 2</F2>
      <F3>Servo 3</F3>
      <F4>Servo 4</F4>
      <F5>Servo 5</F5>
      <F6>Servo 6</F6>
      <F7>Servo 7</F7>
      <F8>Servo 8</F8>
    </GSRV>
    <GNAV>
      <F1>GPS Fix</F1>
      <F2>GPS Numsat</F2>
      <F3>GPS DirToHome</F3>
      <F4>GPS DistToHome</F4>
    </GNAV>
    <GPAR>
      <F1>CycleTime</F1>
      <F2>I2C Errors</F2>
      <F3>VBatt</F3>
      <F4>Power Sum</F4>
    </GPAR>
    <GDBG>
      <F1>Debug 1</F1>
      <F2>Debug 2</F2>
      <F3>Debug 3</F3>
      <F4>Debug 4</F4>
    </GDBG>

    <!-- Log fields from inFlight DataFlash logging-->
    <CYC>
      <F1>Average</F1>
      <F2>MAX</F2>
    </CYC>
  </MULTIWII>
</LOGFORMAT>